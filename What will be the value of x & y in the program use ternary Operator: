import java.util.*;

public class Coditional {
    public static void main(String[] args){
        Scanner st = new Scanner(System.in);
        System.out.println("Welcome");
         int a = 63, b = 36;
        boolean x = (a > b)? true : false ;

        System.out.println(x);

        int y = (a>b)? a : b ;

        System.out.println(y);
  }
}
